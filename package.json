{
  "name": "cursor-api-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Cursor Admin API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "start:log": "node server.js 2>&1 | tee logs/app.log",
    "start:log:json": "node server.js 2>&1 | tee logs/app.log",
    "parse-pdf": "node server.js --parse-pdf",
    "logs:view": "tail -f logs/app.log",
    "logs:error": "grep -i error logs/app.log",
    "logs:activity": "grep ACTIVITY-BY-MONTH logs/app.log"
  },
  "dependencies": {
    "@opendataloader/pdf": "1.10.0",
    "better-sqlite3": "^11.6.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "multer": "^2.0.2"
  }
}
